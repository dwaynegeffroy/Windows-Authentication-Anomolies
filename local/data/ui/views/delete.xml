<form>
  <label>Delete Analytic</label>
  <fieldset submitButton="true" autoRun="true">
    <input type="text" token="analytic_id">
      <label>Analytic ID</label>
    </input>
    <input type="text" token="parent_process_exec">
      <label>Parent Process Exec</label>
    </input>
    <input type="text" token="parent_process_path">
      <label>Parent Process Path</label>
    </input>
    <input type="text" token="process">
      <label>Process Command Line(process)</label>
    </input>
    <input type="text" token="process_exec">
      <label>Process Exec</label>
    </input>
    <input type="text" token="process_path">
      <label>Process Path</label>
    </input>
    <input type="text" token="score">
      <label>Score</label>
    </input>
    <input type="text" token="severity">
      <label>Severity</label>
    </input>
    <input type="text" token="user">
      <label>User</label>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Delete Analytic</title>
      <table>
        <search>
          <query>|inputlookup process_detections 
| where  process_exec!="$process_exec$" AND process_path!="$process_path$" AND process!="$process$" AND parent_process_exec!="$parent_process_exec$" AND parent_process_path!="$parent_process_path$" AND user!="$user$" AND analytic_id!="$analytic_id$" AND score!="$score$" AND severity!="$severity$"   
| outputlookup process_detections 
</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <input type="text" token="list_process" searchWhenChanged="true">
        <label>Process</label>
        <default>*</default>
      </input>
    </panel>
  </row>
  <row>
    <panel>
      <title>Process List</title>
      <table>
        <search>
          <query>|inputlookup process_detections | search process_exec="$list_process$" OR process_path="$list_process$" OR process="$list_process$" OR parent_process_exec="$list_process$" OR parent_process_path="$list_process$" 
          </query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Duplicates</title>
      <table>
        <search>
          <query>|inputlookup process_detections | stats count by process_exec process_path process parent_process_exec parent_process_path  user  | where count&gt;1
          </query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
</form>